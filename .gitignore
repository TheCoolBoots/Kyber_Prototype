# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/master/Unity.gitignore
#
/KyberPrototype/[Ll]ibrary/
/KyberPrototype/[Tt]emp/
/KyberPrototype/[Oo]bj/
/KyberPrototype/[Bb]uild/
/KyberPrototype/[Bb]uilds/
/KyberPrototype/[Ll]ogs/
/KyberPrototype/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/KyberPrototype/[Mm]emoryCaptures/

# Asset meta data should only be ignored when the corresponding asset is also ignored
!/[Aa]ssets/**/*.meta

# Uncomment this line if you wish to ignore the asset store tools plugin
# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/KyberPrototype/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/KyberPrototype/.vs/

# Gradle cache directory
/KyberPrototype/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/KyberPrototype/ExportedObj/
/KyberPrototype/.consulo/
/KyberPrototype/*.csproj
/KyberPrototype/*.unityproj
/KyberPrototype/*.sln
/KyberPrototype/*.suo
/KyberPrototype/*.tmp
/KyberPrototype/*.user
/KyberPrototype/*.userprefs
/KyberPrototype/*.pidb
/KyberPrototype/*.booproj
/KyberPrototype/*.svd
/KyberPrototype/*.pdb
/KyberPrototype/*.mdb
/KyberPrototype/*.opendb
/KyberPrototype/*.VC.db

# Unity3D generated meta files
/KyberPrototype/*.pidb.meta
/KyberPrototype/*.pdb.meta
/KyberPrototype/*.mdb.meta

# Unity3D generated file on crash reports
/KyberPrototype/sysinfo.txt

# Builds
/KyberPrototype/*.apk
/KyberPrototype/*.aab
/KyberPrototype/*.unitypackage

# Crashlytics generated file
/KyberPrototype/crashlytics-build.properties

# Packed Addressables
#/KyberPrototype/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/KyberPrototype/[Aa]ssets/[Ss]treamingAssets/aa.meta
/KyberPrototype/[Aa]ssets/[Ss]treamingAssets/aa/*